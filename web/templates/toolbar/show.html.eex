<nav class="navbar navbar-default navbar-fixed-bottom">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="#">
        <%= img_tag static_path(ExDebugToolbar.Endpoint, "/images/logo.svg") %>
      </a>
    </div>
    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
      <ul class="nav navbar-nav">
        <li> <%= render "show/_overview.html", request: @request %> </li>
        <%= unless @request.logs == [] do %>
          <li> <%= render "show/_logs.html", logs: @request.logs %> </li>
        <% end %>
        <%= unless @request.ecto == [] do %>
          <li> <%= render "show/_ecto.html", queries: ExDebugToolbar.Request.Ecto.split_inline_and_parallel_queries(@request.ecto) %> </li>
        <% end %>
        <%= unless @request.breakpoints.entries == [] do %>
          <li> <%= render "show/_breakpoints.html", request: @request, breakpoints: @request.breakpoints %> </li>
        <% end %>
        <%= unless @history == [] do %>
          <li> <%= render "show/_history.html", request: @request, history: @history %> </li>
        <% end %>
      </ul>
      <div class="navbar-right navbar-header historic-flag">
        <a class="navbar-brand back-to-current-request text-warning" href="#">Back to Current Request</a>
      </div>
    </div>
  </div>
</nav>
